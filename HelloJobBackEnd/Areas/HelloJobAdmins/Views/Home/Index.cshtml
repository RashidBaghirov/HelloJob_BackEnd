<input hidden class="xitmet" value="@Model.Xidmet" />
<input hidden class="dizayn" value="@Model.Dizayn" />
<input hidden class="muxtelif" value="@Model.Muxtelif" />
<input hidden class="sehiyye" value="@Model.Sehiyye" />
<input hidden class="huquq" value="@Model.Huquq" />
<input hidden class="tehsilelm" value="@Model.TehsilElm" />
