@section Scripts{
<script>
    $(document).ready(function () {
        if ($("#loginSuccess").length) {
            toastr["success"]("Welcome back to us!", "Hello")
        }
        else if ($("#loginError").length) {
            toastr["error"]("Username or password is incorrect!", "Upsss Sorry")
        }
    })
</script>
}