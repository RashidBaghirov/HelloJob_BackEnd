<input hidden class="inzibati" value="@Model.Inzibati" />
<input hidden class="saturdeyorder" value="@Model.SaturdayCvOrders" />
<input hidden class="saturdeyorder" value="@Model.SaturdayVacansOrders" />
<input hidden class="mondayorder" value="@Model.MondayCVOrders" />
